{
  "name": "CPFL",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "start": "react-native start",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "release-apk": "npm run bundle && cd android && ./gradlew assembleRelease",
    "release-bundle": "npm run bundle && cd android && ./gradlew bundleRelease",
    "format": "prettier --check ./src",
    "test": "jest --no-cache",
    "clear": "cd android && ./gradlew clean && cd ../",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
